{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["es6","dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "src/*": [
        "src/*"
      ],
      "config/*":[
        "config/*"
      ],
      "@redux/*": ["@redux/*"],
    },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
